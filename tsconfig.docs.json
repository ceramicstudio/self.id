{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "jsx": "preserve",
    "lib": ["DOM", "ES2020"],
    "skipLibCheck": true
  },
  "include": [
    "./packages/3box-legacy/src",
    "./packages/core/src",
    "./packages/framework/src",
    "./packages/image-utils/src",
    "./packages/react/src",
    "./packages/web/src"
  ],
  "typedocOptions": {
    "name": "Self.ID SDK",
    "readme": "none",
    "entryDocument": "overview.md",
    "entryPoints": [
      "./packages/3box-legacy/src/index.ts",
      "./packages/core/src/index.ts",
      "./packages/framework/src/index.ts",
      "./packages/image-utils/src/index.ts",
      "./packages/react/src/index.ts",
      "./packages/web/src/index.ts"
    ],
    "githubPages": false,
    "disableSources": true,
    "excludeExternals": true,
    "excludeInternal": true,
    "excludePrivate": true,
    "hideBreadcrumbs": true,
    "hideInPageTOC": true
  }
}
