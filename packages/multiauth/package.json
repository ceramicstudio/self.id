{
  "name": "@self.id/multiauth",
  "version": "0.1.0",
  "author": "3Box Labs",
  "license": "(Apache-2.0 OR MIT)",
  "homepage": "https://github.com/ceramicstudio/self.id#readme",
  "keywords": [
    "self.id",
    "web3",
    "react",
    "authentication"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ceramicstudio/self.id",
    "directory": "packages/multiauth"
  },
  "main": "dist/lib.cjs",
  "module": "dist/lib.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    "./assets/*": "./assets/*",
    ".": {
      "import": "./dist/lib.mjs",
      "require": "./dist/lib.cjs",
      "default": "./dist/lib.cjs"
    }
  },
  "files": [
    "assets",
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "scripts": {
    "build": "del dist && tsc --emitDeclarationOnly --skipLibCheck && rollup -c ../../rollup.config.js",
    "lint": "eslint --fix",
    "test": "jest",
    "prepare": "yarn build",
    "prepublishOnly": "package-check"
  },
  "dependencies": {
    "@portis/web3": "^4.0.5",
    "@toruslabs/torus-embed": "^1.13.4",
    "@walletconnect/ethereum-provider": "^1.6.6",
    "caip": "^0.9.2",
    "fortmatic": "^2.2.1",
    "jotai": "^1.3.9"
  },
  "peerDependencies": {
    "grommet": "^2.18.0",
    "react": "^17.0.2"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
