{
  "name": "@self.id/core",
  "version": "0.1.0",
  "author": "3Box Labs",
  "license": "(Apache-2.0 OR MIT)",
  "homepage": "https://github.com/ceramicstudio/self.id#readme",
  "keywords": [
    "self.id",
    "web3"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ceramicstudio/self.id",
    "directory": "packages/core"
  },
  "main": "dist/lib.cjs",
  "module": "dist/lib.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    "import": "./dist/lib.mjs",
    "require": "./dist/lib.cjs",
    "default": "./dist/lib.cjs"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "scripts": {
    "model": "node ./scripts/publish-model.mjs",
    "build": "del dist && tsc --emitDeclarationOnly --skipLibCheck && rollup -c ../../rollup.config.js",
    "lint": "eslint --fix",
    "test": "jest",
    "prepare": "yarn build",
    "prepublishOnly": "package-check"
  },
  "dependencies": {
    "@ceramicnetwork/3id-did-resolver": "^1.3.1",
    "@ceramicnetwork/http-client": "^1.1.1",
    "@ceramicnetwork/stream-caip10-link": "^1.1.1",
    "@glazed/datamodel": "0.1.0",
    "@glazed/did-datastore": "0.1.0",
    "caip": "^0.9.2",
    "did-resolver": "^3.1.0",
    "key-did-resolver": "^1.3.1"
  },
  "devDependencies": {
    "@datamodels/identity-accounts-crypto": "^0.1.2",
    "@datamodels/identity-accounts-web": "^0.1.2",
    "@datamodels/identity-profile-basic": "^0.1.2",
    "@glazed/types": "^0.1.2"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  }
}
