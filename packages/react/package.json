{
  "name": "@self.id/react",
  "version": "0.1.0",
  "author": "3Box Labs",
  "license": "(Apache-2.0 OR MIT)",
  "homepage": "https://github.com/ceramicstudio/self.id#readme",
  "keywords": [
    "self.id",
    "web3",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ceramicstudio/self.id",
    "directory": "packages/react"
  },
  "main": "dist/lib.cjs",
  "module": "dist/lib.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    "import": "./dist/lib.mjs",
    "require": "./dist/lib.cjs",
    "default": "./dist/lib.cjs"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "scripts": {
    "build": "del dist && tsc --emitDeclarationOnly --skipLibCheck && rollup -c ../../rollup.config.js",
    "lint": "eslint --fix",
    "test": "jest",
    "prepare": "yarn build",
    "prepublishOnly": "package-check"
  },
  "dependencies": {
    "@self.id/core": "^0.2.0",
    "@self.id/web": "^0.2.0",
    "jotai": "^1.4.3",
    "react-query": "^3.33.0"
  },
  "devDependencies": {
    "@glazed/did-datastore": "^0.2.1",
    "@glazed/types": "^0.1.3"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
