    "prepare": "yarn build"
  },
  "dependencies": {
    "@datamodels/identity-accounts-web": "^0.1.2",
    "@datamodels/identity-profile-basic": "^0.1.2",
    "@self.id/core": "^0.3.0",
    "@self.id/image-utils": "^0.2.0",
    "@self.id/multiauth": "^0.2.0",
    "@self.id/react": "^0.2.0",
    "@self.id/ui": "^0.2.0",
    "@self.id/web": "^0.3.0",
    "boring-avatars": "^1.5.8",
    "grommet": "^2.19.1"
  },
  "devDependencies": {
    "@glazed/types": "^0.1.3"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "jest": {
    "preset": "ts-jest",
