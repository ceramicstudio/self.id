{
  "compilerOptions": {
    "module": "ES2020",
    "lib": ["ES2020"],
    "target": "ES2020",
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "jsx": "react"
  }
}
